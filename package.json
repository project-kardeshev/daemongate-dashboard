{
  "name": "daemongate-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "clean": "rimraf dist && rm -f dist-*",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "lint:check": "eslint .",
    "preview": "vite preview",
    "prepare": "husky || true",
    "pre-commit": "lint-staged",
    "vis": "yarn vite-bundle-visualizer"
  },
  "dependencies": {
    "@fontsource/cormorant-infant": "^5.2.6",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-router": "^1.114.29",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.484.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.17",
    "tw-animate-css": "^1.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.17",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@tanstack/eslint-plugin-router": "^1.114.29",
    "@tanstack/react-router-devtools": "^1.114.29",
    "@tanstack/router-plugin": "^1.114.29",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "commitlint": "^19.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{ts,tsx,js,md,json}": [
      "prettier --write"
    ]
  }
}
